      "color": ""
    },
    "x.com": {
      "selector": "section > div[aria-label='Timeline: Conversation'] > div > div:first-child div[dir='auto'] > span",
      "color": ""
    },
    "www.pinterest.com": {
      "selector": "div > div > div > div[data-test-id='author-and-comment-container'] span.text-container",
